{
    "name": "opsmatic",
    "version": "0.1.2",
    "source": "https://github.com/opsmatic/puppet-opsmatic",
    "author": "Opsmatic",
    "license": "Apache-2.0",
    "project_page": "https://github.com/opsmatic/puppet-opsmatic",
    "summary": "A Puppet module for installing Opsmatic reporter on Puppet runs",
    "operatingsystem_support": [
      {
